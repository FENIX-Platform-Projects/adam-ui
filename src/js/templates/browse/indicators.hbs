<!-- Indicators -->
        {{#grouped_each 3 data.indicators}}
        <div class="row">
            {{#each this }}
                  <div class="col-sm-4 adam-entry-point-content">
                     <div class="col-sm-8"><p>{{this.name}} ({{this.period}}) <sup>{{this.footnote}}</sup></p></div>
                     <div class="col-sm-4"><p>{{decimal this.value}}</p></div>
                  </div>
            {{/each}}
        </div>
         {{/grouped_each}}

           <div class="row">
            <div class="col-sm-1"><p>Source:</p></div>
           {{#each data.footnotes}}
                <div class="col-sm-{{divideBy12 ../data.footnotes.length}}"><p>{{this.footnote}}  {{this.source}}</p></div>
            {{/each}}
          </div>

</div>
