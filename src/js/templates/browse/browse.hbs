<h1 class="browse-title">{{title}}</h1>

<div class="row">

    <div id="side-browse" class="col-xs-12">


        <div class="row">
            <div class="col-xs-12">
                <div class="obj-box">
                   <div class='fx-filter-label'>Choose Topic</div>
                    <div id="browse-topic-selector" class="browse-topic-selector"></div>

                    <br>

                    <h2 class="filter-opener-browse"> {{ filter }}</h2>

                    <div id="filter-browse" class="filter-container">
                    </div>
                    <button id="filter-submit-btn-browse" class="btn btn-default"> {{filter_btn}}</button>
                </div>

            </div>

        </div>


        <div class="row">
            <div class="col-xs-12">
                <h1 class="browse-analysis-title-results">Results</h1>

                <div id="browse-topic-content">
                    dashboard
                </div>

            </div>
        </div>

    </div>


</div>
