<li class="fx-analysis-item fit fx-result" data-status="loading">

    <div class="fx-handle"></div>

    <div class="fx-dataset-details-container">
        <!-- Result Title -->
        <span class="fx-dataset-details truncate">{{oda}}</span>

        <ul class="nav nav-tabs small-icons-analysis" role="tablist">

            <li role="presentation" class="pull-right">
                <a data-control='remove'>
                    <span class="fx-widget-icons icojam_button_delete_1"></span>
                </a>
            </li>
        </ul>

    </div>

    <div class='fx-widget row fit-height'>

        <div class="col-xs-12 fit-height">

            <div data-content="ready" class="fixed-height">

                <ul class="nav nav-tabs small-icons-analysis" role="tablist">

                    <li role="presentation">
                        <a href="#fx-tab-table-{{id}}" role="tab" data-toggle="tab" data-visualization="table">
                            {{ tab_table }}
                        </a>
                    </li>

                    <li role="presentation" data-visualization="chart">
                        <a href="#fx-tab-chart-{{id}}" role="tab" data-toggle="tab" data-visualization="chart">
                            {{ tab_chart }}
                        </a>
                    </li>

                    <li role="presentation" data-visualization="chart">
                        <a href="#fx-tab-info-{{id}}" role="tab" data-toggle="tab" data-visualization="info">
                            {{ tab_info }}
                        </a>
                    </li>

                    <li role="presentation" class="pull-right">
                        <a data-control='download'>
                            <span class="fx-widget-icons icojam_download_1"></span>
                        </a>
                    </li>

                </ul>

                <div class="tab-content fit-height">

                    <div id="fx-tab-table-{{id}}" role="tabpanel" data-visualization="table"
                         class="tab-pane fade fit-height">
                        <div data-container="table" id="fx-tab-table-container-{{id}}">
                        </div>
                    </div>

                    <div id="fx-tab-chart-{{id}}" role="tabpanel" data-visualization="chart"
                         class="tab-pane fade fit-height">

                        <div data-container="chart" class="fit-height"></div>
                    </div>

                    <div id="fx-tab-info-{{id}}" role="tabpanel" data-visualization="info"
                         class="tab-pane fade fit-height">

                        <h3>{{ info_current_selection }}</h3>

                        <dl class="dl-horizontal">

                            <dt>{{info_title_oda}}</dt>
                            <dd>{{info_oda}}</dd>

                            <dt>{{info_title_recipient}}</dt>
                            <dd>
                                {{#if info_recipient}}
                                    {{info_recipient}}
                                {{else}}
                                    -
                                {{/if}}
                            </dd>

                            <dt>{{info_title_donor}}</dt>
                            <dd>
                                {{#if info_donor}}
                                    {{info_donor}}
                                {{else}}
                                    -
                                {{/if}}
                            </dd>

                            <dt>{{info_title_delivery}}</dt>
                            <dd>
                                {{#if info_delivery}}
                                    {{info_delivery}}
                                {{else}}
                                    -
                                {{/if}}
                            </dd>

                            <dt>{{info_title_sector}}</dt>
                            <dd>
                                {{#if info_sector}}
                                    {{info_sector}}
                                {{else}}
                                    -
                                {{/if}}
                            </dd>

                            <dt>{{info_title_sub-sector}}</dt>
                            <dd>
                                {{#if info_sub-sector}}
                                    {{info_sub-sector}}
                                {{else}}
                                    -
                                {{/if}}
                            </dd>

                            <dt>{{info_title_year_from}} - {{info_title_year_to}}</dt>
                            <dd>{{info_year-from}} - {{info_year-to}}</dd>

                        </dl>

                        <button data-control='download' class="btn btn-default pull-right"> {{download}}</button>

                    </div>
                </div>

            </div>

            <div data-content="error" class="fixed-height">
                    <span class="status-label">
                        {{ result_error }}
                        <br><br>
                        <button data-control="reload" class="btn btn-default">{{ reload }}</button>
                    </span>
            </div>

            <div data-content="loading" class="fixed-height">
                <span class="loading loading-48"></span>
            </div>

            <div data-content="empty" class="fixed-height">

                <span class="status-label">
                    {{ result_empty }}
                </span>

                <h3>{{ info_current_selection }}</h3>

                <dl class="dl-horizontal">

                    <dt>{{info_title_oda}}</dt>
                    <dd>{{info_oda}}</dd>

                    <dt>{{info_title_recipient}}</dt>
                    <dd>
                        {{#if info_recipient}}
                            {{info_recipient}}
                        {{else}}
                            -
                        {{/if}}
                    </dd>

                    <dt>{{info_title_donor}}</dt>
                    <dd>
                        {{#if info_donor}}
                            {{info_donor}}
                        {{else}}
                            -
                        {{/if}}
                    </dd>

                    <dt>{{info_title_delivery}}</dt>
                    <dd>
                        {{#if info_delivery}}
                            {{info_delivery}}
                        {{else}}
                            -
                        {{/if}}
                    </dd>

                    <dt>{{info_title_sector}}</dt>
                    <dd>
                        {{#if info_sector}}
                            {{info_sector}}
                        {{else}}
                            -
                        {{/if}}
                    </dd>

                    <dt>{{info_title_sub-sector}}</dt>
                    <dd>
                        {{#if info_sub-sector}}
                            {{info_sub-sector}}
                        {{else}}
                            -
                        {{/if}}
                    </dd>

                    <dt>{{info_title_year_from}} - {{info_title_year_to}}</dt>
                    <dd>{{info_year-from}} - {{info_year-to}}</dd>

                </dl>
            </div>

        </div>

    </div>

</li>